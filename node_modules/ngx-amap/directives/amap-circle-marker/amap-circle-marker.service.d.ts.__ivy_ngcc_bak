/// <reference types="@types/amap-js-api" />
import { NgZone } from '@angular/core';
import { AMapService } from '../../shared/amap.service';
import { LoggerService } from '../../shared/logger/logger.service';
import { Getter } from '../../base/interfaces';
export declare class AmapCircleMarkerService implements Getter<AMap.CircleMarker> {
    private amaps;
    private logger;
    private ngZone;
    private circle;
    private circle$;
    constructor(amaps: AMapService, logger: LoggerService, ngZone: NgZone);
    /**
     * 获取圆点标记
     */
    get(): import("rxjs").Observable<AMap.CircleMarker<any>>;
    /**
     * 创建圆点标记 AMap.CircleMarker
     * @param options 选项
     */
    create(options: AMap.Circle.Options): import("rxjs").Observable<AMap.CircleMarker<any>>;
    /**
     * 销毁圆点标记
     */
    destroy(): void;
}
