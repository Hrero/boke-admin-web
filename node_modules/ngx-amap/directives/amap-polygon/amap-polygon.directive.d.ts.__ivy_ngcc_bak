/// <reference types="@types/amap-js-api" />
import { OnDestroy, SimpleChanges, OnChanges, NgZone } from '@angular/core';
import { AMapPolygon } from '../../base/amap-polygon';
import { AmapPolygonService } from './amap-polygon.service';
import { LoggerService } from '../../shared/logger/logger.service';
import { EventBinderService } from '../../shared/event-binder.service';
export declare class AmapPolygonDirective extends AMapPolygon<AMap.Polygon, any> implements OnChanges, OnDestroy {
    protected os: AmapPolygonService;
    protected binder: EventBinderService;
    private logger;
    private ngZone;
    private inited;
    constructor(os: AmapPolygonService, binder: EventBinderService, logger: LoggerService, ngZone: NgZone);
    ngOnDestroy(): void;
    ngOnChanges(changes: SimpleChanges): void;
    /**
     * 获取已创建的 AMap.Polygon 对象
     */
    get(): import("rxjs").Observable<any>;
}
